    <script>
        seajs.use('homepage',function(m){
            m.game();
        })
    </script>

    <script>
        seajs.use(['scrollLoading'],function ($){
            $(function(){
                $('.scrollLoading').scrollLoading();
            });
        })
    </script>

   <!------------ jQuery轮播插件模块化测试 ----------------->
    <script>
        seajs.use(['slideBox'],function ($){
            $(function(){
                    $('#demo1').slideBox();
            });
        });
    </script>


<!------------- 框架模板 ---------------->
    <!--id可以用来唯一确定一个模版,type是模版固定的写法-->
    <!-- swiper中silder模板 -->
    <script id="slider_modle" type="text/x-handlebars-template">
        {{#each this}}
        <div class="swiper-slide">
            <a href="{{game_url}}">
                <img class="scrollLoading"  data-url="{{game_advertising_map}}" alt="">
                <!--<img class="samLazyImg" src="{{game_advertising_map}}" alt="">-->
            </a>
        </div>
        {{/each}}
    </script>

    <!-- 我的收藏列表项模板 -->
    <script id="colliction_li_template" type="text/x-handlebars-template">
        {{#each hot_game}}
        <li>
            <div>
                <img class="scrollLoading" data-url="{{game_icon}}">
                <div class="coll-game-des">
                    <span class="coll-game1-name">{{game_name}}</span>

                    <span class="coll-game1-num">{{number}}人在玩</span>
                    <span class="coll-game1-des">{{game_desp}}</span>
                </div>
            </div>
        </li>
        {{/each}}
    </script>

    <!-- 热门游戏列表项模板 -->
    <script id="card_template" type="text/x-handlebars-template">
        {{#each hot_game}}
        <div class="hot-game1">
            <img class="scrollLoading" data-url="{{game_icon}}">
            <div class="hot-game1-des">
                <a class="back-or-close">
                     <span>关闭</span>
                    <span class="icon icon-share"></span>
                </a>
                <span class="hot-game1-name title-font-size">{{game_name}}</span>
                <div class="hot-game1-des-keyword">
                    {{#with game_keyword}}
                        {{#each this}}
                             <span class=>{{this}}</span>
                        {{/each}}
                    {{/with}}

                    <span class="hot-game1-num">{{number}}人在玩</span>
                </div>
                <span class="little-font-size coll-game1-advertisement">{{game_desp}}</span>
            </div>
        </div>
        {{/each}}

    </script>



