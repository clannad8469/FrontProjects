    <style>
        /* Pagination Styles */
        .my-swiper-pagination{
            text-align: right;
        }

        .swiper-pagination-bullet {
            width: 8px;
            height: 8px;
            display: inline-block;
            border-radius: 100%;
            background: #fff;
            opacity: 0.2;
        }
        .swiper-pagination-bullet-active {
            opacity: 1;
            background: #007aff;
        }

        .swiper-container-horizontal > .swiper-pagination {
            bottom: 0;
            left: 0;
            width: 100%;
        }

    </style>
